{
  "name": "pano-video-call",
  "version": "1.7.2",
  "private": true,
  "scripts": {
    "dev": "cross-env IS_ELECTRON=true PANO_SERVE_ENV=dev vue-cli-service electron:serve",
    "dev:web": "cross-env PANO_SERVE_ENV=dev vue-cli-service serve",
    "build": "cross-env IS_ELECTRON=true vue-cli-service electron:build -p never",
    "build:web": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "pkg:mac": "cross-env PLATFORM=mac USE_HARD_LINKS=false npm run build ",
    "pkg:win": "cross-env PLATFORM=win npm run build",
    "pkg:linux": "yarn build && electron-builder build --linux"
  },
  "main": "background.js",
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@pano.video/panortc": "2.2.10",
    "@pano.video/panortc-electron-sdk": "^1.3.1",
    "@pano.video/panorts": "^2.4.3",
    "@types/crypto-js": "^4.0.2",
    "ant-design-vue": "^1.7.8",
    "babel-loader": "^8.2.3",
    "core-js": "^3.6.5",
    "crypto-js": "^4.1.1",
    "electron": "11.5.0",
    "ua-parser-js": "^1.0.2",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/less": "^3.0.3",
    "@types/node": "12",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron-devtools-installer": "^3.1.0",
    "electron-notarize": "^1.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^4.1.1",
    "less-loader": "7.3.0",
    "native-ext-loader": "^2.3.0",
    "prettier": "^2.2.1",
    "typescript": "~4.1.5",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "no-undef": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-var-requires": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".babelrc",
          ".eslintrc",
          ".stylelintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  }
}
